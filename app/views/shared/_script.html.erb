      }
    })
  })
  $('#file4').change(function(e) {
    e.preventDefault()
    $('#uploader-form-4').ajaxSubmit({
      success: function() {
        $('#file4_trigger').text('Done!')
        $('#file4_trigger').addClass('done')
        checkFormReadyness()
      },
      uploadProgress: function(event, position, total, percentComplete) {
        $('#file4_trigger').text(percentComplete + '% uploaded')
      }
    })
  })


  $('.modal .snap-submit').click(function(e){
    e.preventDefault()
    el = e.currentTarget.id.split('-')[0] + '-form';
    var obj = $("#".concat(el))
    obj.ajaxSubmit({
      success: function() {
        $('.modal').modal('hide');
        location.reload();
      }
    })
  })


  var swiper = new Swiper('.swiper-container', {
      slidesPerView: 3,
      spaceBetween: 20,
      freeMode: true,
      loop: true,
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
      },
    });
  })
