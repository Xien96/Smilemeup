<div class="home product container text-center">
  <div class="row eq-height">
    <div class="col-md-3 col-xs-12 col-sm-push-1 steps-slogan steps-slogan-left">
      <div class="slogan-width">
        <h3 class=""><%= t('products.steps_title1')%> <br><span class="green"><strong><%= t('products.steps_title2')%> </strong></span></h3>
      </div>
      <p class="slogan-des">
        <a class="btn btn-default btn-lg btn-green" href="/products/snapup/customize" role="button">
          <%= t('products.snap_up.order_now')%>
        </a>
      </p>
    </div>
    <div class="col-md-6 col-xs-12 col-sm-push-2">
      <div class="row">
        <div class="col-md-12 col-sm-8 col-xs-12 shadow product-steps-item steps-slogan-left">
          <div class="cell choose-product">
            <div class="col-md-4 col-sm-4" style="margin: 0">
            <%= image_tag('choosedesign.svg') %>
          </div>
          <div class="col-sm-8 col-sm-8 product-text">
            <h4 class="choose-heading"><%= t('products.step1')%> </h4>
          </div>
          </div>
        </div>
        <div class="col-md-12 col-sm-8 col-xs-12 shadow product-steps-item steps-slogan-left">
          <div class="cell choose-product">
            <div class="col-md-4 col-sm-4" style="margin: 0">
            <%= image_tag('order-clearup.svg') %>
          </div>
          <div class="col-sm-8 col-sm-8 product-text">
          <h4 class="choose-heading"><%= t('products.step2')%><br><strong>SNAP UP.</strong></h4>
        </div>
          </div>
        </div>
        <div class="col-md-12 col-sm-8 col-xs-12 shadow product-steps-item steps-slogan-left">
          <div class="cell choose-product">
            <div class="col-md-4 col-sm-4" style="margin: 0">
            <%= image_tag('impressions.svg') %>
            </div>
            <div class="col-sm-8 col-sm-8 product-text">
            <h4 class="choose-heading"><%= t('products.step3')%> <strong> <%= t 'products.step4' %> </strong></h4>
          </div>
          </div>
        </div>
        <div class="col-md-12 col-sm-8 col-xs-12 shadow product-steps-item steps-slogan-left">
          <div class="cell choose-product">
            <div class="col-md-4 col-sm-4" style="margin: 0">
            <%= image_tag('welcome.svg') %>
          </div>
          <div class="col-sm-8 col-sm-8 product-text">
            <h4 class="choose-heading"><%= t('products.welcome')%> <strong><%= t('products.new_smile')%></strong></h4>
          </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

